<script src="mraid.js"></script>
    <div class="hyperv1.0">
        <img src="data:image/png,hyperv1.0" style="display: none" onerror="
            (function(img) {
                var params = {'${PARTNER_MACROS}','extAdServer':'ad-server'};
                var req = document.createElement('script');
                req.id = params.scriptId = 'hyper-script-' + (window.hyperScriptIndex = (window.hyperScriptIndex||0)+1);
                params.clientTimestamp = new Date/1000;
                params.clientTimeZoneOffsetInMinutes = new Date().getTimezoneOffset();
                params.hostPageLoadId=window.hyperHostPageLoadId=window.hyperHostPageLoadId||(Math.random()+'').slice(2);
                var qs = 'guid=${TAG_GUID}';
                for (var k in params) {
                    qs += '&amp;' + encodeURIComponent(k) + '=' + encodeURIComponent(params[k]);
                }
                var src = '${DOMAIN}' + qs;
                req.src = src;
                img.parentNode.insertBefore(req, img.nextSibling);
            })(this);
        "/>
    </div>